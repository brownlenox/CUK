enum 50101 AcademicProgressStatus
{
    Extensible = true;

    value(0; "")
    {
        Caption = 'Empty';
    }

    value(1; Active)
    {
        Caption = 'Active';
    }
    value(2; Inactive)
    {
        Caption = 'Inactive';
    }
    value(3; Graduated)
    {
        Caption = 'Graduated';
    }
}
